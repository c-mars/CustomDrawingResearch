<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TestProgressBar">
        <attr name="inactiveColor" format="color" />
        <attr name="activeColor" format="color" />
        <attr name="doneColor" format="color" />
        <attr name="stepsCount" format="integer" />
        <attr name="currentProgress" format="integer" />
    </declare-styleable>

    <declare-styleable name="ShapeView">
        <attr name="shapeColor" format="enum">
            <enum name="white" value="0"/>
            <enum name="black" value="1"/>
            <enum name="red" value="2"/>
            <enum name="green" value="3"/>
            <enum name="blue" value="4"/>
            <enum name="yellow" value="5"/>
            </attr>
        <attr name="shapeType" format="enum">
            <enum name="square" value="0"/>
            <enum name="triangle" value="1"/>
            <enum name="circle" value="2"/>
            </attr>
    </declare-styleable>
</resources>